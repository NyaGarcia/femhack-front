{
  "name": "femhack-the-googler",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "db:create": "ts-node --project scripts/tsconfig.json scripts/falso.ts",
    "server:start": "json-server --nc true --watch ./api/database.json --routes ./api/routes.json -m ./node_modules/json-server-auth",
    "front:start": "ng serve --proxy-config proxy.conf.json -o ",
    "femhack": "concurrently --kill-others \"npm run server:start\" \"npm run front:start\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.3",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.3",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@ngneat/falso": "^2.8.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concurrently": "^7.0.0",
    "jasmine-core": "~3.10.0",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.2"
  }
}
